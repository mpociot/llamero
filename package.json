{
  "name": "Llamero",
  "version": "1.0.1",
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": "Marcel Pociot",
  "homepage": "https://pociot.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/mpociot/llamero.git"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "build:mac-arm64": "electron-builder install-app-deps --platform=darwin --arch=arm64 && npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config"
  },
  "dependencies": {
    "7zip-min": "^1.4.4",
    "@electron-toolkit/preload": "^1.0.3",
    "@electron-toolkit/utils": "^1.0.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "electron-log": "^5.0.0-beta.16",
    "electron-updater": "^5.3.0",
    "fix-path": "3.0.0",
    "isomorphic-git": "^1.22.0",
    "node-pty": "^0.10.1",
    "nodejs-file-downloader": "^4.10.6",
    "semver": "^7.3.8",
    "tar": "^6.1.13",
    "xterm": "^5.1.0"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "16.18.14",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "autoprefixer": "^10.4.14",
    "electron": "^23.0",
    "electron-builder": "^23.6.0",
    "electron-vite": "^1.0.20",
    "eslint": "^8.36.0",
    "eslint-plugin-vue": "^9.9.0",
    "less": "^4.1.3",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vue": "^3.2.47",
    "vue-tsc": "^1.2.0"
  }
}
